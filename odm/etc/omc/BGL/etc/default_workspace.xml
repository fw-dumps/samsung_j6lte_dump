<?xml version="1.0" encoding="utf-8" ?>
<favorites xmlns:launcher="http://schemas.android.com/apk/res-auto/com.android.launcher3">
    <home>
        <!--  screen [1]  -->
        <!-- appwidget : Weather -->
        <appwidget
            launcher:screen="0"
            launcher:packageName="com.sec.android.daemonapp"
            launcher:className="com.sec.android.daemonapp.appwidget.WeatherAppWidget2x1"
            launcher:x="1"
            launcher:y="0"
            launcher:spanX="3"
            launcher:spanY="3" />

        <!-- appwidget : Google app -->
        <appwidget
            launcher:screen="0"
            launcher:packageName="com.google.android.googlequicksearchbox"
            launcher:className="com.google.android.googlequicksearchbox.SearchWidgetProvider"
            launcher:x="0"
            launcher:y="3"
            launcher:spanX="5"
            launcher:spanY="1" />

        <!-- Calendar -->
        <favorite
            launcher:packageName="com.samsung.android.calendar" 
            launcher:className="com.android.calendar.AllInOneActivity" 
            launcher:screen="0" 
            launcher:x="0" 
            launcher:y="4" 
            launcher:spanX="1" 
            launcher:spanY="1" />

        <!-- Gallery -->
        <favorite
            launcher:packageName="com.sec.android.gallery3d" 
            launcher:className="com.sec.android.gallery3d.app.GalleryOpaqueActivity" 
            launcher:screen="0" 
            launcher:x="1" 
            launcher:y="4" 
            launcher:spanX="1" 
            launcher:spanY="1" />

        <!-- Play Store -->
        <favorite
            launcher:packageName="com.android.vending" 
            launcher:className="com.android.vending.AssetBrowserActivity"  
            launcher:screen="0" 
            launcher:x="2" 
            launcher:y="4" 
            launcher:spanX="1" 
            launcher:spanY="1" />

        <!-- Folder : Google -->
        <folder
            launcher:screen="0" 
            launcher:title="Google" 
            launcher:x="3" 
            launcher:y="4"
            launcher:hideAddButton="true">
            <!-- Google -->
            <favorite
                launcher:screen="0" 
                launcher:packageName="com.google.android.googlequicksearchbox" 
                launcher:className="com.google.android.googlequicksearchbox.SearchActivity" />
            <!-- Chrome -->
            <favorite
                launcher:screen="1" 
                launcher:packageName="com.android.chrome" 
                launcher:className="com.google.android.apps.chrome.Main" />
            <!-- Gmail -->
            <favorite
                launcher:screen="2" 
                launcher:packageName="com.google.android.gm" 
                launcher:className="com.google.android.gm.ConversationListActivityGmail" />
            <!-- Maps -->
            <favorite
                launcher:screen="3" 
                launcher:packageName="com.google.android.apps.maps" 
                launcher:className="com.google.android.maps.MapsActivity" />
            <!-- YouTube -->
            <favorite
                launcher:screen="4" 
                launcher:packageName="com.google.android.youtube" 
                launcher:className="com.google.android.youtube.app.honeycomb.Shell$HomeActivity" />
            <!-- Drive -->
            <favorite
                launcher:screen="5" 
                launcher:packageName="com.google.android.apps.docs" 
                launcher:className="com.google.android.apps.docs.app.NewMainProxyActivity" />
            <!-- Play Music -->
            <favorite
                launcher:screen="6" 
                launcher:packageName="com.google.android.music" 
                launcher:className="com.android.music.activitymanagement.TopLevelActivity" />
            <!-- Play Movies -->
            <favorite
                launcher:screen="7" 
                launcher:packageName="com.google.android.videos" 
                launcher:className="com.google.android.youtube.videos.EntryPoint" />
            <!-- Duo -->
            <favorite
                launcher:screen="8" 
                launcher:packageName="com.google.android.apps.tachyon" 
                launcher:className="com.google.android.apps.tachyon.MainActivity" />
            <!-- Photos -->
            <favorite
                launcher:screen="9" 
                launcher:packageName="com.google.android.apps.photos" 
                launcher:className="com.google.android.apps.photos.home.HomeActivity" />
        </folder>

        <!-- Folder : Microsoft Apps -->
        <folder
            launcher:screen="0" 
            launcher:title="Microsoft Apps"
            launcher:x="4" 
            launcher:y="4" >
            <!-- Word -->
            <favorite
                launcher:screen="0" 
                launcher:packageName="com.microsoft.office.word" 
                launcher:className="com.microsoft.office.apphost.LaunchActivity" />
            <!-- Excel -->
            <favorite
                launcher:screen="1" 
                launcher:packageName="com.microsoft.office.excel" 
                launcher:className="com.microsoft.office.apphost.LaunchActivity" />
            <!-- PowerPoint -->
            <favorite
                launcher:screen="2" 
                launcher:packageName="com.microsoft.office.powerpoint" 
                launcher:className="com.microsoft.office.apphost.LaunchActivity" />
            <!-- OneDrive -->
            <favorite
                launcher:screen="3" 
                launcher:packageName="com.microsoft.skydrive" 
                launcher:className="com.microsoft.skydrive.MainActivity" />
            <!-- LinkedIn -->
            <favorite
                launcher:screen="4" 
                launcher:packageName="com.linkedin.android"
                launcher:className="com.linkedin.android.authenticator.LaunchActivity" />
        </folder>

    </home> 

    <!-- Hotseat (We use the screen as the position of the item in the hotseat) -->
    <hotseat>
        <!-- Phone -->
        <favorite
            launcher:screen="0"
            launcher:packageName="com.samsung.android.contacts"
            launcher:className="com.android.dialer.DialtactsActivity" />

        <!-- Contacts -->
        <favorite
            launcher:screen="1"
            launcher:packageName="com.samsung.android.contacts"
            launcher:className="com.android.contacts.activities.PeopleActivity" />

        <!-- Messages -->
        <favorite
            launcher:screen="2"
            launcher:packageName="com.samsung.android.messaging"
            launcher:className="com.android.mms.ui.ConversationComposer" />

        <!-- Internet -->
        <favorite
            launcher:screen="3"
            launcher:packageName="com.sec.android.app.sbrowser"
            launcher:className="com.sec.android.app.sbrowser.SBrowserMainActivity" />

        <!-- Camera -->
        <favorite
            launcher:screen="4"
            launcher:packageName="com.sec.android.app.camera"
            launcher:className="com.sec.android.app.camera.Camera" />

    </hotseat> 

</favorites> 
